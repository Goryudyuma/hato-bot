{
  "name": "hato-bot",
  "description": "愛嬌のあるslack用bot",
  "website": "https://github.com/nakkaa/hato-bot",
  "repository": "https://github.com/nakkaa/hato-bot",
  "env": {
    "SLACKBOT_API_TOKEN": {
      "description": "Set your slack API Token.",
